<interface>
  <object id="window" class="GtkWindow">
  <property name="default-width">500</property>
  <property name="default-height">300</property>
  <property name="resizable">0</property>
    <property name="visible">True</property>
    <property name="title">Mickye - Remote Administration Tool</property>
    <property name="border-width">10</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="row-spacing">20</property>
        <property name="visible">True</property>
        <child>
          <object id="start" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Start server</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="stop" class="GtkButton">
            <property name="visible">True</property>
            <property name="sensitive">False</property>
            <property name="label">Stop server</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="quit" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Quit</property>
          </object>
          <packing>
            <property name="left-attach">3</property>
            <property name="top-attach">0</property>
            <property name="width">1</property>
          </packing>
        </child>
      </object>
      <packing>
      </packing>
    </child>
  </object>
</interface>